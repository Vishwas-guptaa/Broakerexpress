<div class="container mt-5">
<div class="bg-light rounded p-3">
                  <div class="bg-white rounded p-4" style="border: 1px dashed rgba(0, 185, 142, .3)">
  <div class="mg-t-100">
    <h1 class="bhh1">Showing Best Property For You</h1>
      </div>
      <div class="snav">
        <ul class="snav-style">
          <li><a href="" class="snav-item">Home
          <i class="fa fa-arrow-right"></i></a></li>
          <li><a href="" class="snav-item">&nbsp;Property</li></a>
          <li><a href="" class="snav-item active">&nbsp;/&nbsp;Most Popular Property In Gwalior </a></li>
        </ul>
      </div>
          <h4 class="bhh2 mb-5">showing a best Room & Flat for you !!</h4>
            <div class="row ">

             <% r.forEach((item)=>{ %>
              <div class="col-lg-4 col-md-6 mb-3">
                    
                <div class="imgbox">
                    <a href="/details/<%- item._id %>"><img class="img-fluid" src="<%= item.f_image.url %> " style="width: 500px;height: 250px;"   alt=""></a>
                    <div class="bg-success rounded text-white position-absolute start-0 top-0 m-4 py-1 px-3"><%= item.flatfor %></div>
                    <div class="bg-success rounded text-white position-absolute end-0 bottom-0 m-4 py-0 px-1">
                    <a class="btn btn-success" href=""><%= item.status %></a>
                  </div>
                    <div class="bg-white rounded-top text-success position-absolute start-0 bottom-0 mx-4 pt-1 px-3"><%= item.type %></div>
                </div>
                <div class="p-4 pb-0">
                    <h5 class="text-success mb-3"><%= item.prize %>â‚¹</h5>
                    <a class="d-block h5 mb-2 text-decoration-none" href=""><%= item.flatname %></a>
                    <p><i class="fa fa-map-marker-alt text-danger me-2"></i><%= item.location %></p>
                </div>
                <div class="d-flex border-top">
                    <small class="flex-fill text-center border-end py-2"><i class="fa fa-ruler-combined text-success me-2"></i><%= item.area %></small>
                    <small class="flex-fill text-center border-end py-2"><i class="fa-solid fa-hand-holding-heart" style="color: #198754;"></i> <%= item.facility %></small>
                    <small class="flex-fill text-center py-2"><i class="fa fa-bath text-success me-2"></i><%= item.bath %></small>
                </div>
                <div class="text-center">
                    <a class="btn btn-success mt-3" href="/details/<%- item._id %>">View details</a>
                  </div>
            </div>
              <% }) %>

               

