<%-include('../../admin/sidebar.ejs')  %>
<%- include('../../admin/header.ejs')  %>

<div class="content mt-3">
    <div class="animated fadeIn">
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-header d-flex justify-content-between">
                        <div>
                          <strong class="card-title text-success">client Book Table</strong>
                        </div>
                       
                    </div>
                    <div class="card-body">
                        <table id="bootstrap-data-table-export" class="table table-striped table-bordered">
                            <thead class="bg-success text-white">
                                <tr>          
                                    <th>Name</th>
                                    <th>Mobile</th>
                                     <th>address</th>
                                     <th>propertyname</th>
                                     <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% read.forEach((item)=>{ %>
                                <tr>
                                  <td><%= item.name %></td>
                                  <td><%= item.mobile %></td>
                                  <td><%= item.address %></td> 
                                  <td><%= item.propertyname %></td>   
                                  <td> <a href="/admin/bookdelete/<%= item._id %>" class="btn btn-danger">Delete</a></td>
                              </tr>

                              
                              <% }) %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div><!-- .animated -->
  
  
  </div><!-- .content -->
  </div>
